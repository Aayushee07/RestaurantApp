<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFE5BF"
    tools:context=".mealbowlmenupage"
    android:orientation="vertical"
    >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:background="#DB5C00"
                android:fontFamily="@font/archivo_black"
                android:gravity="center"
                android:text="Meal Bowls"
                android:textColor="#FAF7F7"
                android:textSize="30sp" />
            <TextView
                android:id="@+id/textView11"
                android:layout_width="match_parent"
                android:layout_height="116dp"
                android:layout_marginTop="10dp"
                android:backgroundTint="#FFFFFF"
                android:gravity="center"
                android:text=""
                app:drawableTint="#DC9A9A" />
            <ImageView
                android:id="@+id/imageView7"
                android:layout_width="136dp"
                android:layout_height="127dp"
                android:layout_marginTop="-120dp"
                app:srcCompat="@drawable/indian_wrestler" />
            <TextView
                android:id="@+id/textView14"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="150dp"
                android:layout_marginLeft="150dp"
                android:layout_marginTop="-100dp"
                android:layout_marginEnd="20dp"
                android:layout_marginRight="20dp"
                android:text="Indian Wrestler"
                android:textSize="16sp"
                android:textStyle="bold" />
            <TextView
                android:id="@+id/textView15"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="150dp"
                android:layout_marginLeft="150dp"
                android:layout_marginEnd="150dp"
                android:layout_marginRight="150dp"
                android:text="Price:150Rs"
                android:textSize="16sp"
                android:textStyle="bold" />


            <Button
                android:id="@+id/button14"
                android:layout_width="167dp"
                android:layout_height="37dp"
                android:layout_marginStart="150dp"
                android:backgroundTint="#DD9326"
                android:text="ADD TO CART" />

            <TextView
                android:id="@+id/textView17"
                android:layout_width="match_parent"
                android:layout_height="101dp"
                android:layout_marginTop="25dp"
                android:text="" />

            <ImageView
                android:id="@+id/imageView10"
                android:layout_width="139dp"
                android:layout_height="98dp"
                android:layout_marginTop="-100dp"
                app:srcCompat="@drawable/super_bowl" />


            <TextView
                android:id="@+id/textView18"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="150dp"
                android:layout_marginLeft="150dp"
                android:layout_marginTop="-90dp"
                android:layout_marginEnd="20dp"
                android:layout_marginRight="20dp"
                android:text="Mom's Super Bowl"
                android:textSize="16sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView19"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="150dp"
                android:layout_marginLeft="150dp"
                android:layout_marginEnd="150dp"
                android:layout_marginRight="150dp"
                android:text="Price:140Rs"
                android:textSize="16sp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/button9"
                android:layout_width="167dp"
                android:layout_height="37dp"
                android:layout_marginStart="150dp"
                android:layout_marginLeft="150dp"
                android:layout_marginEnd="130dp"
                android:layout_marginRight="130dp"
                android:gravity="center"
                android:text="Add to cart"
                app:backgroundTint="#DD9326" />
            <TextView
                android:id="@+id/textView20"
                android:layout_width="match_parent"
                android:layout_height="116dp"
                android:layout_marginTop="25dp"
                android:text="" />

            <ImageView
                android:id="@+id/imageView11"
                android:layout_width="143dp"
                android:layout_height="99dp"
                android:layout_marginTop="-115dp"
                app:srcCompat="@drawable/thai_bowl" />

            <TextView
                android:id="@+id/textView21"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="150dp"
                android:layout_marginLeft="150dp"
                android:layout_marginTop="-90dp"
                android:layout_marginEnd="20dp"
                android:layout_marginRight="20dp"
                android:text="Thai Bowl "
                android:textSize="16sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView22"
                android:layout_width="match_parent"
                android:layout_height="20dp"
                android:layout_marginStart="150dp"
                android:layout_marginLeft="150dp"
                android:layout_marginEnd="150dp"
                android:layout_marginRight="150dp"
                android:text="Price:100Rs"
                android:textSize="16sp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/button10"
                android:layout_width="170dp"
                android:layout_height="38dp"
                android:layout_marginStart="150dp"
                android:layout_marginLeft="150dp"
                android:layout_marginEnd="130dp"
                android:layout_marginRight="130dp"
                android:gravity="center"
                android:text="Add to cart"
                app:backgroundTint="#DD9326" />

<!--            <Button-->
<!--                android:id="@+id/button"-->
<!--                android:layout_width="167dp"-->
<!--                android:layout_height="40dp"-->
<!--                android:layout_marginStart="120dp"-->
<!--                android:layout_marginLeft="150dp"-->
<!--                android:layout_marginTop="30dp"-->
<!--                android:layout_marginEnd="130dp"-->
<!--                android:layout_marginRight="130dp"-->
<!--                android:gravity="center"-->
<!--                android:text="Logout"-->
<!--                app:backgroundTint="#DB5C00" />-->


        </LinearLayout>
    </LinearLayout>

</RelativeLayout>